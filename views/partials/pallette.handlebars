<div class="pallette-wrapper">
    <div class="pallette">
        <div class="toolbar-title">
            Tools
        </div>
        <div class="pallette-grid">
            <button id="tool-delete-button" class="pallette-item">
                <img src="/images/icons8-delete-48.png" alt="">
                <span>Delete</span>
            </button>
        </div>
        <div class="toolbar-title border-top">
            Layout
        </div>
        <div class="pallette-grid">
                {{#each layoutbuttons}}
                    {{> layoutbuttons}}
                {{/each}}
        </div>
        <div class="toolbar-title border-top">
            Objects
        </div>
        <div class="pallette-grid">
            {{#each objects}}
                {{> objects}}
            {{/each}}
        </div>
    </div>
</div>